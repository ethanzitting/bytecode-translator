// Executes pop and push commands using the
// pointer, this, and that segments.
push constant 3030
    // Stack has 3030
pop pointer 0
    // Stack empty, this (RAM[3]) has 3030
push constant 3040
    // Stack has 3040
pop pointer 1
    // Stack empty, that (RAM[4]) has 3040
push constant 32
    // Stack has 32
pop this 2
    // Stack empty, RAM[3032] = 2
push constant 46
    // Stack has 46
pop that 6
    // Stack empty, RAM[3046] = 46
push pointer 0
    //
push pointer 1
add
push this 2
sub
push that 6
add
